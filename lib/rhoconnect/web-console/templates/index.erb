	<script language="javascript" type="text/javascript" src='models/user.js'></script>	  	
	<script language="javascript" type="text/javascript" src='models/client.js'></script>	  	
	<script language="javascript" type="text/javascript" src='models/stats.js'></script>
	<script language="javascript" type="text/javascript" src='public/bootstrap-collapse.js'></script>
</head>
<body>
	<input id='token' type='hidden' value='<%=@token%>'>
	<input id='domain' type='hidden' value='<%=@domain%>'>
	<div id='heroku_nav'><%=@heroku%></div>
	<div id='nav_menu' class="navbar navbar-fixed-top" style="<%@heroku ? 'display:none' : 'display:block'%>"> 
		  <div class='navbar-inner'>
			<div class='container'>
				<div class="brand" style='padding:5px 0 0 0;margin-left:0;font-size:26px'>
					<img src='public/logo.png' alt='image' height='40' width='40'/>
					<div style='font-family:Motofont;display:inline-block'>RHO<div style='color:#c91a1d;display:inline'>CONNECT</div></div>
				</div> 
				<ul class='nav pull-right' style='margin-top:10px'> 
				    <li class=""><a href="/resque/" >Resque</a></li>
				    <li class="active"><a href="#" >Console</a></li> 
					<li><a href='http://docs.rhomobile.com/rhoconnect/introduction' target='_blank'>Docs</a></li>
					<li><a id='logout' style='display:none' href="#logout">Logout</a></li>
					<li><a id='login' href="#">Login</a></li>
				</ul>
		     </div><!-- END container -->
	      </div><!-- END nav inner -->
	</div><!-- END navbar -->
		<div class='container'>
			<!-- secondary nav after login         -->
			<div id='secondary-nav' class='span12' style='display:none;margin-top:70px;margin-left:0'>
				<ul class='nav nav-tabs'>
					<li id='info_home'><a href='#'>Home</a></li>
					<li id='server_doc'><a href='#docselect'>Server Doc</a></li>
					<li id='users'><a href='#users'>Users</a></li>
					<% link = Rhoconnect.stats == true ? '#stats' : '#'%>
					<% l_klass = Rhoconnect.stats == true ? '' : 'disable_link'%>
					<li id='stats'><a class='<%=l_klass%>' href='<%=link%>'>Statistics</a></li>
				</ul>
			</div>
			<div class='span12' style='margin-left:0'>
				<!-- content goes here -->
				<div id='main_content'>
				</div>
				<!-- content ends here -->
			</div>
		</div>
	<!--END row --> 
	<div class='container'>
	<hr>
	<footer>
		<div class="pull-left">
			RhoConnect
