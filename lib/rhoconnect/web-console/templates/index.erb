	<script language="javascript" type="text/javascript" src='views/set_adapter.js'></script>	  	
	<script language="javascript" type="text/javascript" src='views/server_doc.js'></script>	  	
	<script language="javascript" type="text/javascript" src='views/users.js'></script>	  	
	<script language="javascript" type="text/javascript" src='views/new_user.js'></script>	  	
